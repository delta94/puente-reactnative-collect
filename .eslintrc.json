{
    "extends": [
        "airbnb"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-use-before-define": "off",
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "comma-dangle": "off",
        "react/no-string-refs":"off",
        "react/no-access-state-in-setstate":"off"
      },
    "env": {
        "jest": true,
        "browser": true
    }
}